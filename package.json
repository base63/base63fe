        "nodemon": "=1.12.5",
        "raynor-chai": "=0.1.1",
        "raw-loader": "=0.5.1",
        "remap-istanbul": "=0.9.5",
        "style-loader": "=0.19.0",
        "supertest": "=3.0.0",
