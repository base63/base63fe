        "@types/continuation-local-storage": "=3.2.1",
        "@types/cookie-parser": "=1.4.1",
        "@types/dotenv": "=4.0.2",
        "@types/express": "=4.0.39",
        "@types/http-status-codes": "=1.0.29",
        "@types/knex": "=0.0.67",
        "@types/mocha": "=2.2.44",
        "@types/mustache": "=0.8.30",
        "@types/react": "=16.0.29",
        "@types/react-dom": "=16.0.3",
        "@types/react-helmet": "=5.0.3",
        "@types/node": "=8.0.57",
        "@types/serialize-javascript": "=1.3.2",
        "@types/supertest": "=2.0.4",
        "@types/uuid": "=3.4.3",
        "@types/webpack": "=3.8.1",
        "@types/webpack-dev-middleware": "=1.9.4",
        "chai": "=4.1.2",
        "codecov": "=3.0.0",
