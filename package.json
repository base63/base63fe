{
    "name": "@base63/base63fe",
    "version": "0.0.0",
    "description": "The frontend to the base63.com application - editor, landing pages, about etc",
    "author": "Base63 team",
    "scripts": {
        "build": "tsc && webpack -p",
        "serve-dev": "mkdir -p out && touch out/server/index.js && (tsc --watch & nodemon out/server/index.js) | bunyan --output short",
        "serve-prod": "node out/server/index.js | bunyan --output simple",
        "test": "tsc && istanbul cover --config ./.istanbul.yml ./node_modules/mocha/bin/_mocha --  $(find out -name '*-test.js') && remap-istanbul -i ./build/coverage/coverage-final.json -o ./build/coverage/coverage-remapped.json && coverage-filter -i ./build/coverage/coverage-remapped.json -o ./build/coverage/coverage-filtered.json",
        "push-coverage-to-codecov": "codecov -f ./build/coverage/coverage-filtered.json",
        "docs": "typedoc src/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/base63/base63fe.git"
    },
    "license": "MIT",
    "homepage": "https://github.com/base63/base63fe#readme",
    "filesPack": {
        "out/client": "c:client",
        "out/server": "c:server",
        "webpack.config.js": "c:webpack.config.js",
        "package.json": "c:package.json",
        "tsconfig.json": "c:tsconfig.json",
        "tsconfig.client.json": "c:tsconfig.client.json"
    },
    "dependencies": {
        "@base63/common-js": "=0.2.2",
        "@base63/common-server-js": "=1.5.1",
        "@base63/identity-sdk-js": "=0.5.1",
        "@base63/content-sdk-js": "=0.0.2",
        "async-middleware": "=1.2.1",
        "compression": "=1.7.1",
        "cookie-parser": "=1.4.3",
        "express": "=4.16.2",
        "continuation-local-storage": "=3.2.1",
        "http-status-codes": "=1.3.0",
        "knex": "=0.14.2",
        "mustache": "=2.3.0",
        "pg": "=7.4.0",
        "raynor": "=0.14.0",
        "react": "=16.2.0",
        "react-dom": "=16.2.0",
        "react-helmet": "=5.2.0",
        "serialize-javascript": "=1.4.0",
        "uuid": "=3.1.0",
        "webpack-dev-middleware": "=1.12.2"
    },
    "devDependencies": {
        "@base63/togemfury": "=0.1.0",
        "@types/bluebird": "=3.5.18",
